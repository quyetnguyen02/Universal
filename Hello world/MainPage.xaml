        <RadioButton Content="Female&#xD;&#xA;" Margin="206,75,0,0" Grid.Row="2" VerticalAlignment="Top"/>
        <Button Content="Login" Margin="66,265,0,0" Grid.Row="2" VerticalAlignment="Top" RenderTransformOrigin="2.153,0.578"/>
        <Button Content="Reset" Margin="169,265,0,0" Grid.Row="2" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="66,112,0,0" Grid.Row="2" Text="UserName" TextWrapping="Wrap" VerticalAlignment="Top"/>

    </Grid>
</Page>
